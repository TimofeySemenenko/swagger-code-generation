{
  "name": "TimofeySemenenko/swagger-code-generation",
  "description": "Swagger scheme for laravel project",
  "license": "proprietary",
  "authors": [
    {
      "name": "Semenenko Timofey, Telman Mazhlumov",
      "email": "semenenko.tim@yandex.ru"
    }
  ],
  "keywords": [
    "framework",
    "laravel",
    "lumen",
    "swagger",
    "API",
    "SOA"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "nette/php-generator": "^3.2",
    "vlucas/phpdotenv": "^3.3",
    "wn/lumen-generators": "^1.3"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "App\\": "app/",
      "\\Src\\SwaggerCodeGeneration\\": "Src/SwaggerCodeGeneration/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}